    dez deneme = için(satır <- girdi) ver(satır.böl(" ").eleDeğilse(_.boyu == 0))
    dez bilgi = deneme.yazıYap("", "\n", "")
    // satıryaz(bilgi)
    // todo: şimdilik..
    dez n = eğer (noktalar.boyu == 0) {
        Nokta(10, 20) // yeni bir nokta oluştur
    } yoksa noktalar.başı
    Dizin( (n, (10, 20)), (n, (20, 10)) )
}

